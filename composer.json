{
    "name": "upwind-media/symfony-launchpad",
    "description": "Symfony Launchpad - Ultimate Symfony CLI Docker & K8S Helper.",
    "homepage": "https://upwind-media.github.io/symfony-launchpad",
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/console": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/process": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/config": "^5.1",
        "symfony/filesystem": "^5.1",
        "novactive/collection": "^1.1.0",
        "symfony/options-resolver": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "nesbot/carbon": "^2"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.4",
        "mikey179/vfsstream": "^1.6",
        "twig/twig": "^3",
        "symfony/translation": "^5.1",
        "symfony/twig-bridge": "^5.1"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        }
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Launchpad\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "prefer-stable": true,
    "bin": [
        "bin/sf"
    ]
}
